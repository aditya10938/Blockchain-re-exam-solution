pragma solidity ^ 0.4.23;
contract TrainTicket{
    string public PassengerName;
    int public Passengerage;
    string public Passengergender;
    int public PassengerSeatNo;
    int public PassengerCoachNo;
    string public ArrivalStationCode;
    string public DestinationStationCode;
    string public DateofJourney;

    function newPassengertrainticket(string newPassengerName, int newPassengerage, string newPassengergender, int newPassengerSeatNo, int newPassengerCoachNo, string newPassengerArrivalStationCode, string newPassengerDestinationStationCode, string newPassengerDateofjourney)public{
    PassengerName = newPassengerName;
    Passengerage = newPassengerage;
    Passengergender = newPassengergender;
    PassengerSeatNo = newPassengerSeatNo;
    PassengerCoachNo = newPassengerCoachNo;
    ArrivalStationCode = newPassengerArrivalStationCode;
    DestinationStationCode = newPassengerDestinationStationCode;
    DateofJourney = newPassengerDateofjourney;
    }

    
}
